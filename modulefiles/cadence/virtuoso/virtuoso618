#%Module

proc ModuleHelp { } {
    puts stderr "This module adds Cadence Virtuoso to your path."
}

module-whatis "This module adds Cadence Virtuoso to your path\n"
set basediric "/EDA_Tools/Cadence/IC618"
setenv CDSHOME "${basediric}"
prepend-path PATH $::env(CDSHOME)/tools/bin
prepend-path PATH $::env(CDSHOME)/tools/dfII/bin
setenv OA_HOME "${basediric}/oa_v22.60.092"
prepend-path PATH $::env(OA_HOME)/bin
set basedirspectre "/EDA_Tools/Cadence/Spectre2310"
setenv MMSIMHOME "${basedirspectre}"
prepend-path PATH $::env(MMSIMHOME)/tools/bin
prepend-path PATH $::env(MMSIMHOME)/tools/dfII/bin
prepend-path LM_LICENSE_FILE "5280@cadenceuic.webstore.illinois.edu"
