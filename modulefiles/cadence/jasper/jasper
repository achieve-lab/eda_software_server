#%Module

proc ModuleHelp { } {
    puts stderr "This module adds Cadence JasperGold to your path."
}

module-whatis "This module adds Cadence JasperGold to your path\n"
set basedir "/EDA_Tools/Cadence/Jasper2024/jasper_2024.0620240515043917"
prepend-path PATH "${basedir}/bin"
prepend-path CDS_LIC_FILE "5280@cadenceuic.webstore.illinois.edu"
